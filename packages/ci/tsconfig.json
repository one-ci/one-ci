{
  "compilerOptions": {
    "target": "ESNext",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      // "@/*": ["src/*"]
    },
    "rootDir": ".",
    "types": ["node", "jest", "miniprogram-ci"],
